#include <stdio.h>
#include <locale.h>
int main(){
	setlocale(LC_ALL,"");
	int n;
	printf("informe o tamanho do vetor \n");
	scanf("%d", &n);
	int v[n],aux,x,i;
	
	for(i=0; i<n;i++){
		scanf("%d", &v[i]);
	}
	
	for(i=0;i<n;i++){
		for(x=i+1;x<n;x++){
			if(v[i]>v[x]){
				aux=v[i];
				v[i]=v[x];
				v[x]=aux;
			}
		}
	}
	
	for(i=0;i<n;i++){
		printf("A ordem Ã© %d \n",v[i]);
	}
}
